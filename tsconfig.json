{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["dom", "esnext"],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "stripInternal": true,
    "noUnusedLocals": true
  }
}
